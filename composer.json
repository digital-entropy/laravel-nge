{
    "name": "dentro/nge",
    "description": "Docker helper for Laravel.",
    "keywords": ["laravel", "docker"],
    "license": "MIT",
    "support": {
        "issues": "https://github.com/digital-entropy/nge/issues",
        "source": "https://github.com/digital-entropy/nge"
    },
    "authors": [
        {
            "name": "Aditya Chandra",
            "email": "addeeandra@gmail.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "illuminate/console": "^12.0",
        "illuminate/contracts": "^12.0",
        "illuminate/support": "^12.0",
        "symfony/console": "^7.0",
        "symfony/yaml": "^7.0"
    },
    "require-dev": {
        "orchestra/testbench": "^7.0|^8.0|^9.0",
        "phpstan/phpstan": "^1.10"
    },
    "bin": [
        "bin/nge"
    ],
    "autoload": {
        "psr-4": {
            "Dentro\\Nge\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Dentro\\Nge\\NgeServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
